@page "/admin"

@layout RunTogether.Shared.Layouts.OrganiserLayoutHome

@using RunTogether.Shared.Etc
@using RunTogether.Areas.Identity

@inject RunTogether.Data.ApplicationDbContext dbContext

@attribute [Authorize(Roles = IdentityRoleTypes.Organiser)]

<AuthorizeView>
    <Authorized>
        @*<p>You can only see this content if you're authorized.</p>*@
    </Authorized>
    <NotAuthorized>
        <h1>Authentication Failure!</h1>
        <p>You're not signed in.</p>
    </NotAuthorized>
</AuthorizeView>
<DataTable></DataTable>
